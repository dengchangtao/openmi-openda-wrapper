$*************************HEADING************************

PROJ 's11' '000'

$********************MODEL INPUT*************************

SET NAUTICAL 
SET LEVEL=0;

MODE NONST 

CGRID UNSTRUCTURED CIRCLE 36 0.03 0.5  
READ UNSTRUCTURED TRIANGLE 'rect_6400'

INPGRID BOTTOM REGULAR -1000. -1000. 0. 8 5  8000  5000  EXC -9999.
READINP BOTTOM 1. '../INPUT_files/bathy/flat_20m.BOT' 

INPGRID WIND REG 0. 0. 0 1 1 2000 2000 NONSTAT 20081121.0000 12 HR 20081125.0000 
READINP WIND 1.0 SERIes 'rotating.SER' 3 FREE 

$************************************* BOUNDARY CONDITIONS **************************************

BOUND NEST 'wam_adapted.bnd' OPEN
     
$****************************************** PHYSICA *********************************************

GEN3 WESTH
BREAKING 1 0.73
FRICTION JONSWAP CFJON=0.038

$************************************ NUMERIEKE PARAMETERS ***************************************

PROP BSBT
NUM ACCUR NONSTAT mxitns=20

$************************************ OUTPUT ***************************************

CURVE  'C1' 1000 3000 10 1000 0 
$CURVE  'C2' 2000 3000 10 2000 0 
$CURVE  'C3' 3000 3000 10 3000 0 
$CURVE  'C4' 4000 3000 10 4000 0 
$CURVE  'C5' 5000 3000 10 5000 0 

BLOCK 'COMPGRID' NOHEAD  's11.mat' &
LAYOUT 3 XP YP HSIG DHS HSWELL TMM10 DIR DEPTH &
OUT 20100101.0000 1 HR

TABLE   'C1' HEADER     'out_C1.TAB' TSEC XP YP DEP HSIG DHS HSWELL &
		TMM10 WLEN DIR DSPR WIND GENERAT GENWIND OUT 20100101.0000 1 HR
		
$TABLE   'C2' HEADER     'out_C2.TAB' TSEC XP YP DEP HSIG DHS HSWELL &
$		TMM10 WLEN DIR DSPR WIND GENERAT GENWIND OUT 20100101.0000 1 HR
$
$TABLE   'C3' HEADER     'out_C3.TAB' TSEC XP YP DEP HSIG DHS HSWELL &
$		TMM10 WLEN DIR DSPR WIND GENERAT GENWIND OUT 20100101.0000 1 HR
$
$TABLE   'C4' HEADER     'out_C4.TAB' TSEC XP YP DEP HSIG DHS HSWELL &
$		TMM10 WLEN DIR DSPR WIND GENERAT GENWIND OUT 20100101.0000 1 HR
$
$TABLE   'C5' HEADER     'out_C5.TAB' TSEC XP YP DEP HSIG DHS HSWELL &
$		TMM10 WLEN DIR DSPR WIND GENERAT GENWIND OUT 20100101.0000 1 HR		
		
POINTS 'P1' FILE '../INPUT_files/obs_pts/obs6.txt'

SPECOUT 'P1' SPEC1D ABS 's11.sp1'
SPECOUT 'P1' SPEC2D ABS 's11.sp2'
		
TEST 1 0

COMPUTE NONSTAT &
20100101.0000   60 MIN 20100101.1000 

HOTFILE 's11.HOT'

STOP
