<?xml version="1.0" encoding="UTF-8"?>
<costa xmlns:xi="http://www.w3.org/2001/XInclude">
<!--$URL: https://repos.deltares.nl/repos/openda/openda_1/public/trunk/core/native/examples/oscill/etc/rrsqrt_waqua_oscill.xml $-->
<!--$Revision: 710 $, $Date: 2008-10-30 15:09:38 +0100 (do, 30 okt 2008) $-->

  <!-- Observations used for assimilation -->
  <CTA_SOBS id="obs_assim" database="../obs/obs_oscill.sql"/>

  <!-- Used model class -->
  <CTA_MODELCLASS id="modelclass"  name="CTA_MODBUILD_SP" />

  <!-- Filter configuration -->
  <method name="rrsqrt_waqua">
     <rrsqrt_filter>
     <CTA_TIME id="simulation_span" start="0" stop="50.0" step=".1"/>
     <noise_substate>oscill_model</noise_substate>
     <modes max_id="10">
       <model>
         <xi:include href="oscill_sp.xml"/>
       </model>
    </modes>
     </rrsqrt_filter>
  </method>
</costa>

